[color]
  	ui = auto
	status = auto
	diff = auto
	branch = auto
	interactive = auto
	grep = auto
[push]
	default = upstream
[core]
	autocrlf = input 
	editor = vim
	quotepath = false
	whitespace = trailing-space,space-before-tab,indent-with-non-tab
[merge]
	tool = vimdiff
	ff = false
[branch]
	autosetuprebase = always
[pull]
	rebase = true
[pager]
	log = diff-highlight | less
	show = diff-highlight | less
	diff = diff-highlight | less